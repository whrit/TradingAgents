# Alpaca Test Environment Configuration
# Copy this file to .env.test and add your paper trading credentials

# Paper Trading API Credentials
# Get these from: https://app.alpaca.markets/paper/dashboard/overview
ALPACA_PAPER_KEY=YOUR_PAPER_API_KEY_HERE
ALPACA_PAPER_SECRET=YOUR_PAPER_SECRET_KEY_HERE

# API Configuration
ALPACA_BASE_URL=https://paper-api.alpaca.markets
ALPACA_DATA_URL=https://data.alpaca.markets
ALPACA_STREAM_URL=wss://stream.data.alpaca.markets

# Test Configuration
TEST_SYMBOL=AAPL
TEST_QUANTITY=1
TEST_TIMEOUT=30
MAX_TEST_ORDERS=10

# Logging
LOG_LEVEL=DEBUG
PYTEST_LOG_LEVEL=INFO
